#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/mnt/d/task/server/node_modules/.pnpm/typeorm@0.2.45/node_modules/typeorm/node_modules:/mnt/d/task/server/node_modules/.pnpm/typeorm@0.2.45/node_modules:/mnt/d/task/server/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/mnt/d/task/server/node_modules/.pnpm/typeorm@0.2.45/node_modules/typeorm/node_modules:/mnt/d/task/server/node_modules/.pnpm/typeorm@0.2.45/node_modules:/mnt/d/task/server/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../typeorm/cli.js" "$@"
else
  exec node  "$basedir/../typeorm/cli.js" "$@"
fi
